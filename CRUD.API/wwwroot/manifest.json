{
	"_version": "1.21.0",
	"sap.app": {
		"id": "sap.ui.api.jogadores",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			],
			"fallbackLocale": ""
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.api.jogadores.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.98.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.api.jogadores.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "sap.ui.api.jogadores.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes":[
				{
					"pattern":"",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "detalhes/{id}",
		  			"name": "detalhes",
		  			"target": "detalhes"
				},
				{
					"pattern": "cadastro",
		  			"name": "cadastro",
		  			"target": "cadastro"
				},
				{
					"pattern": "edicao/{id}",
       				"name": "edicao",
        			"target": "edicao"
				}

			],
			"targets":{
				"home":{
					"id": "home",
					"name": "TelaListaDeJogadores"
				},
				"detalhes":{
					"id": "detalhes",
					"name": "Detalhes"
				},
				"cadastro":{
					"id":"cadastro",
					"name":"cadastro"
				},
				"edicao": {
					"id": "edicao",
					"name": "cadastro"
				}
			}
		}
	}
}
